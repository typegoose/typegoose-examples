    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "commitlint": "17.5.1",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.36.0",
