{
  "name": "typegoose-examples",
  "private": true,
  "version": "0.0.0",
  "description": "Example for Integration with typegoose",
  "repository": "https://github.com/typegoose/typegoose-examples",
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
